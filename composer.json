{
    "name": "spatie/elasticsearch-search-string-parser",
    "description": "Build Elasticsearch queries based of a query string",
    "keywords": [
        "spatie"
    ],
    "homepage": "https://github.com/spatie/elasticsearch-search-string-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Vanderbist",
            "email": "alex.vanderbist@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Ruben Van Assche",
            "email": "ruben@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "elasticsearch/elasticsearch": "^7.12",
        "illuminate/collections": "^8.40|^9.0|^10.0|^11.0|^12.0",
        "spatie/elasticsearch-query-builder": "^1.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.2|^7.0|^7.8",
        "larapack/dd": "^1.1",
        "phpunit/phpunit": "^9.3|^10.0|^11.0",
        "spatie/phpunit-snapshot-assertions": "^4.2|^5.0",
        "vimeo/psalm": "^4.4|^5.0|^6.12"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\ElasticsearchStringParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ElasticsearchStringParser\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
